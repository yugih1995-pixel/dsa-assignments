#include <bits/stdc++.h>
using namespace std;

int main() {
    int V = 5;
    vector<vector<pair<int,int>>> adj(V);
    adj[0].push_back({1, 2});
    adj[0].push_back({3, 6});
    adj[1].push_back({0, 2});
    adj[1].push_back({2, 3});
    adj[1].push_back({3, 8});
    adj[1].push_back({4, 5});
    adj[2].push_back({1, 3});
    adj[2].push_back({4, 7});
    adj[3].push_back({0, 6});
    adj[3].push_back({1, 8});
    adj[4].push_back({1, 5});
    adj[4].push_back({2, 7});

    vector<int> key(V, INT_MAX), mstSet(V, 0), parent(V, -1);
    key[0] = 0;
    for (int count = 0; count < V - 1; count++) {
        int u = -1;
        for (int i = 0; i < V; i++)
            if (!mstSet[i] && (u == -1 || key[i] < key[u])) u = i;
        mstSet[u] = 1;
        for (auto [v, w] : adj[u])
            if (!mstSet[v] && w < key[v]) {
                key[v] = w;
                parent[v] = u;
            }
    }
    int total = 0;
    for (int i = 1; i < V; i++) total += key[i];
    cout << "MST Cost: " << total;
}
